                        <thead style="color:white; background-color:#E84133">
                            <tr>
                                <th width="200px">Name</th>
                                <th width="200px">E-mail</th>
                                @*<th width="120px">Landline</th>*@
                                <th width="200px">Job title</th>
                                <th width="200px">Team</th>
                                <th width="200px">Department</th>
                                <th width="100px">Office</th>
                                @*<th width="180px">Working hours</th>*@
                            </tr>
                        </thead>
                        <tbody>
                            @for (var i = 0; i < Model.AllEmployees.Count(); i++)
                            {
                                <tr>
                                    <td><a asp-controller="HR" asp-action="EmployeeProfile" asp-route-employeeToViewId="@Model.AllEmployees.ElementAt(i).Id">@Model.AllEmployees.ElementAt(i).FirstName @Model.AllEmployees.ElementAt(i).Surname</a> @(Model.AllEmployees.ElementAt(i).IsTeamManager == true ? "(Team Manager)" : "")</td>
                                    <td>@Model.AllEmployees.ElementAt(i).EmailAddress</td>
                                    @*<td>@Model.AllEmployees.ElementAt(i).LandlineNumber</td>*@
                                    <td>@Model.AllEmployees.ElementAt(i).JobTitle</td>
                                    <td>@Model.AllEmployeeTeamNames.ElementAt(i)</td>
                                    <td>@Model.AllEmployeeDepartmentNames.ElementAt(i)</td>
                                    <td>@Model.AllEmployeeOfficesNames.ElementAt(i)</td>
                                    @*<td>@Model.AllEmployees.ElementAt(i).WorkingHoursNotes</td>*@
                                </tr>
                            }
                        </tbody>

                    </table>
